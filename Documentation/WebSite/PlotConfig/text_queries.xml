<phedex>

  <import file="$CONFIG_ROOT/phedex_queries.xml" />
  <import file="$CONFIG_ROOT/data_queries.xml" />

  <import module="phedex.tools.query_output">QueryPrinter,PhedexXmlGenerator</import>

  <class type="QueryPrinter" name="query">
    <!-- Prints results of queries to stdout, slightly formatted. -->
    <queryobj> PhedexQueries </queryobj>
    <queryobj> DataQueries </queryobj>
  </class>

  <class type="PhedexXmlGenerator" name="query_xml">
    <queryobj> PhedexQueries </queryobj>
    <queryobj> DataQueries </queryobj>
  </class>

</phedex>
